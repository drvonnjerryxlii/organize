<%= form_for :session, role: "form", html: { class: "form-inline" } do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, placeholder: "shark1724@shark.com", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password, placeholder: "Super Secret Password", class: "form-control" %>
  </div>

  <%= f.submit "Login", class: "btn btn-default" %>
<% end %>
